@model List<Redis.UI.Entities.Photo>

@{
    ViewData["Title"] = "Index";
}

<h1>Photos</h1>
<br />
<a href="/Home/AddMongo">New Photo</a>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Url</th>
            <th scope="col">Thumbnail Url</th>
            <th scope="col">Album Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.title</td>
                <td>@item.url</td>
                <td>@item.thumbnailUrl</td>
                <td>@item.albumId</td>
            </tr>
        }
    </tbody>
</table>


